{
    "name": "vscode-theme-dusky",
    "displayName": "Dusky Theme",
    "description": "A theme extension for VS Code that uses a monotone color scheme.",
    "version": "0.0.1",
    "publisher": "mwalker-us",
    "scripts": {
        "vsce" : "vsce",
        "publish": "vsce publish",
        "package": "vsce package "
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mwalker-us/vscode-theme-dusky"
    },
    "engines": {
        "vscode": "*"
    },
    "categories": [
        "Themes"
    ],
    "icon": "icon.png",
    "contributes": {
        "themes": [
            {
                "label": "Dusky",
                "uiTheme": "vs-dark",
                "path": "./themes/dusky.json"
            }
        ]
    },
    "dependencies": {
        "vsce": "^1.100.2"
    }
}
